        <icon src="www/img/logo72.png" width="72" height="72" />
        <icon src="www/img/logo144.png" width="144" height="144" />
        <icon src="www/img/logo29.png" width="29" height="29" />
        <icon src="www/img/logo58.png" width="58" height="58" />
        <icon src="www/img/logo50.png" width="50" height="50" />
